

<div class="container">
  <div class="hero-header">
    <div class="main-text">Pair BnB</div>
    <div class="sub-text">A place to find your next home</div>
    <input type="text" placeholder="Try 'Kuala Lumpur'"><button type="submit">Search</button>
  </div>

<%  @listings.each do |listing| %>
<div class="col-md-12">
  <div class="well">
          <p> <%= listing.id %> </p>
          <p> <%= listing.title %> </p>     
          <p> <%= listing.address %> </p>
          <p> <%= listing.price %> </p>
          <p> <%= listing.status %> </p>
          <p> <%= link_to 'Show', listing_path(listing) %> </p>

          <% if signed_in? and current_user.moderator? %>
          <p> <%= link_to 'Verify', edit_listing_path(listing) %> </p>
          <% end %>  
<% end %>
</div>
</div>


<br>
<div class="pagination">
    <div class="page_info">
      <%= page_entries_info @listings %>
    </div>
    <%= will_paginate @listings, :container => false %>
  </div>


  <div class="div-one">
    <div class="para-one">
      <h1>About Us</h1>
    A place to host or rent a great place.</div>
    <div class="para-two">
      <h1>Why use us</h1>
      Convenient and safe.
    </div>
  </div>
</div>
<div class="pusher"></div>
<div id="footer">
  <ul>
    <li><%= link_to "About Us", about_path %></li>
    <li>Privacy</li>
    <li>Terms</li>
    <li>Contact Us</li>
    <li>Signup for newsletter</li>
    <li>Â© Pairbnb, Inc.</li>
  </ul>
</div>

<p><a href="#">Back to top</a></p>