<table>
<tr>
<th>Title</th>
<th>Address</th>
<th>Price</th>
</tr>

<%  @listings.each do |listing| %>
    <tr>
          <td> <%= listing.title %> </td>     
          <td> <%= listing.address %> </td>
          <td> <%= listing.price %> </td>
          <td> <%= link_to 'Show', listing_path(listing) %> </td>
    </tr>   
<% end %>
</table>