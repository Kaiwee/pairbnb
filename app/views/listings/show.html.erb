<h1>
	<strong>Title:</strong>
	<%= @listing.title %>		
</h1>

<h1>
	<strong>Address:</strong>
	<%= @listing.address %>		
</h1>

<h1>
	<strong>Price:</strong>
	<%= @listing.price %>		
</h1>

<h1>
	<%= @listing.max_num_guests %> guests maximum	
</h1>

<h1>
	<strong>Status:</strong>
	<%= @listing.status %>		
</h1>

<h1>
	<% if @listing.photos? %>
	<strong>Pictures</strong><br>
		<% @listing.photos.each do |photo| %>
			<%= image_tag(photo) %>
		<% end %>
	<% end %>	
</h1>

<% if current_user.id != @listing.user_id %>
<div class="well">
<%= render 'reservations/form' %>
</div>
<% end %>